doctype html
html
    block vars
    head
        meta(charset='utf-8')
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport" content="width=device-width")
        link(rel='icon' href='../../../img/favicon.ico' type='image/x-icon')

        link(rel='stylesheet', href="../../../css/style.css")

        title #{title}

        script if (/*@cc_on!@*/false) {document.documentElement.className += ' ie10';}
        script(src="../../../js/app-common.js")
        script(src="../../../js/app.js")
        script(src="../../../js/jquery.pixlayout.0.9.6.js")

    include mixins

    body
        .page-wrap
            include header
            block content
            .psevdofooter
            include  footer

